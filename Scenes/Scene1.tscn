var choise = randi_range(1,4)
var choise2 
var character= load(\"res://Objects/Character.tscn\").instantiate()
@onready var cam = $Camera2D



func _on_timer_timeout():
	choise = randi_range(1,4)



func _on_timer_2_timeout():

	var enemy = preload(\"res://Objects/rigid_body_2d.tscn\").instantiate()
	add_child(enemy)
	
	var randx1 = randi_range(-467,1835)
	var randx2 = randi_range(-1000,1835)
	var randx3 = randi_range(-1100,1067)
	var randy1 =  randi_range(-775,1490)
	var randy2 = randi_range(-775,1057)
	var randy3 = randi_range(-457,1490)
	var enemy_position

	choise2 = randi_range(1,3)
	
	if choise == 1:
		if choise2 == 1:
			enemy_position = Vector2(randx1,1490)
		elif choise2 == 2:
			enemy_position = Vector2(1835,randy1)
		elif choise2 == 3:
			enemy_position = Vector2(randx1,-775)
	elif choise == 2:
		if choise2 == 1:
			enemy_position = Vector2(1835,randy2)
		elif choise2 == 2:
			enemy_position = Vector2(randx2,-775)
		elif choise2 == 3:
			enemy_position = Vector2(-1100,randy2)
	elif choise == 3:
		if choise2 == 1:
			enemy_position = Vector2(randx3,-775)
		elif choise2 == 2:
			enemy_position = Vector2(-1100,randy1)
		elif choise2 == 3:
			enemy_position = Vector2(randx3,1490)
	elif choise == 4:
		if choise2 == 1:
			enemy_position = Vector2(-1100,randy3)
		elif choise2 == 2:
			enemy_position = Vector2(randx2,1490)
		elif choise2 == 3:
			enemy_position = Vector2(1835,randy3)
	enemy.position = enemy_position + cam.position
